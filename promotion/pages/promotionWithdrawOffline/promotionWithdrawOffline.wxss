page{
  background-color: #e9e9e9;
}
.withdraw-by-other-dialog {
  background-color: #fff;
  margin: 20rpx;
  border-radius: 8rpx;
  color: #333;
  font-size: 28rpx;
  padding-bottom:66rpx;
  box-shadow: 0 0 5rpx rgba(90,95,103,0.21);
}
.withdraw-by-other-title {
  background:#f3f3f3;
  width: calc(100% - 100rpx);
  padding: 0 50rpx;
  line-height: 134rpx;
  height: 134rpx;
  border-radius:8rpx 8rpx 0 0;
}
.withdraw-by-other-title text {
  margin-left: 50rpx;
  color: #ff7100;
}
.widthdraw-form {
  line-height: 74rpx;
  height: auto;
  padding: 20rpx;
  text-align: center;
}
.widthdraw-form label {
  width: 180rpx;
  display: inline-block;
  vertical-align: top;
  text-align: left;
}
.widthdraw-form input,
.widthdraw-form picker{
  display: inline-block;
  width: 425rpx;
  border: 1px solid #d5d5d5;
  border-radius: 8rpx;
  background-color: #f3f3f3;
  vertical-align: top;
  height:76rpx;
  text-align: left;
  text-indent:5px;
  position: relative;
}
.widthdraw-form textarea{
  display: inline-block;
  width: 425rpx;
  height: 130rpx;
  border: 1px solid #d5d5d5;
  border-radius: 8rpx;
  background-color: #f3f3f3;
  vertical-align: top;
  text-align: left;
  text-indent:10rpx;
  line-height: 42rpx;
}
.bank-tip {
  color: red;
  text-indent: 244rpx;
}
.withdraw-by-other-dialog .payattention {
  color: #999;
  font-size: 24rpx;
  text-align: left;
  width: 605rpx;
  padding: 23rpx;
  margin: 0 auto;
}
.withdraw-by-other-dialog button {
  background-color: #ff7100;
  color: #fff;
  width: 610rpx;
  font-size: 30rpx;
  text-align: center;
  margin-top: 30rpx;
  padding: 0;
  height: 80rpx;
  line-height: 80rpx;
}
.withdrawSuccess {
  width: 560rpx;
  height: 300rpx;
  background-color: #fff;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -255rpx;
  margin-left: -280rpx;
  text-align: center;
  font-size: 28rpx;
  border-radius: 8rpx;
  color: #000;
  z-index: 10;
}
.icon-success-tip{
  padding-top: 70rpx;
}
.icon-success-tip:before {
  color: #56c538;
  margin-right: 30rpx;
}
.close-withdraw-btn {
  border-top: 1px solid #d2d2d2;
  padding: 30rpx 0;
  color: #ff7100;
  font-size: 34rpx;
  margin-top: 52rpx;
}