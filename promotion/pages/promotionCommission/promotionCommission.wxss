.table-header {
  display: -webkit-flex;
  display: flex;
  padding: 34rpx 0;
  line-height: 34rpx;
  text-align: center;
  color: #fff;
  background-color: #ff7100;
}
.table-header view {
  -webkit-flex: 1;
  flex: 1;
  font-size: 28rpx;
  border-right: 1px solid #ff8b50;
}
.table-header view:last-child {
  border-right: none;
}
.table-section {
  margin: 20rpx 0;
  font-size: 29rpx;
  color: #666;
}
.table-row {
  display: -webkit-flex;
  display: flex;
}
.table-row .completed {
  color: #FF7100;
}
.even-row {
  background-color: #FFF4EC;
}
.table-cell {
  -webkit-flex: 1;
  flex: 1;
  padding: 0 20rpx;
  line-height: 80rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
}
.navigator-footer {
  position:fixed;
  bottom:0;
  left:0;
  right:0;
  font-size:28rpx;
  text-align:center;
  background:#fff;
  padding:20px 0;
}
.navitaor-btn {
  margin: 0 10px;
  display: inline-block;
  width: 118rpx;
  height: 56rpx;
  line-height: 56rpx;
  border-radius: 4px;
  color: #fff;
  background-color: #ff7100;
}
.navigator-btn.disbaled {
  background-color: #dedede;
}
.creat-time {
  -webkit-flex:1;
  flex:1;
  padding:0 20rpx;
  text-align:center;
  font-size: 12px;
  line-height: 40rpx;
}
.table-row:nth-child(even){
  background: #FFF4EC;
}
.orange {
  color: #FF7100;
}
