page {
  background-color: #f4f4f4;
}
.top-section {
  padding-top: 20rpx;
  font-size: 28rpx;
  text-align: center;
  color: #fff;
  background-color: #ff7100;
}
.commission-detail {
  display: -webkit-flex;
  display: flex;
  padding: 20rpx 0;
  color: #ffc3a5;
  background: #E95C02;
}
.commission-detail-item {
  -webkit-flex: 1;
  flex: 1;
  padding: 10rpx 0;
  border-right: 1px solid #F05B10;
}
.commission-detail-item view:first-child {
  margin-bottom: 10rpx;
}
.middle-section {
  margin-bottom: 42rpx;
  font-size: 32rpx;
}
.withdraw-input-container {
  margin-bottom: 32rpx;
  line-height: 96rpx;
  background-color: #fff;
}
.withdraw-input-container text {
  margin-left: 22rpx;
  display: inline-block;
  vertical-align: middle;
  width: 210rpx;
}
.withdraw-input-container input {
  display: inline-block;
  vertical-align: middle;
  margin-left: 30rpx;
  width: 450rpx;
}
.middle-section-item {
  position: relative;
  margin-bottom: 10rpx;
  padding-left: 70rpx;
  font-size: 28rpx;
  color: #666;
}
.middle-section-item:before {
  content: '';
  position: absolute;
  left: 35rpx;
  top: 6rpx;
  display: block;
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  border: 5rpx solid #ff7100;
}
.bottom-section {
  margin-top: 68rpx;
}
.withdraw-btn {
  margin: 0 auto;
  display: block;
  width: 690rpx;
  height: 88rpx;
  line-height: 88rpx;
  border-radius: 8rpx;
  text-align: center;
  color: #fff;
  background-color: #ff7100;
}
.success-dialog {
  margin: 0 auto;
  width: 750rpx;
  height: 750rpx;
  text-align: center;
  background-size: 100%;
}
.success-dialog-content {
  margin: -125rpx auto 0;
  background-color: #fff;
  height:638rpx;
  width: 560rpx;
  border-radius: 14rpx;
  position: relative;
  top: 50%;
}
.success-dialog-content image {
  width: 100%;
  height: 328rpx;
}
.success-tip {
  margin: 40rpx auto 40rpx;
  font-size: 33rpx;
}
.success-dialog-content button {
  font-size: 28rpx;
  width: 400rpx;
  color: #fff;
  line-height: 80rpx;
  background-color: #ff7100;
  border-color: #ff7100;
  margin: 40rpx auto;
  border: 1px solid #ff7100;
}
.dialog-close {
  position:absolute;
  top:6rpx;
  right:6rpx;
  display:inline-block;
  width:40rpx;
  height:40rpx;
  color:1px solid #999;
  border-radius:50%;
  color:rgba(255,255,255,0.5);
  text-align:center;
  line-height:40rpx;
  font-size:40rpx;
}
.withdrawSuccess {
  width: 560rpx;
  height: 300rpx;
  background-color: #fff;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -150rpx;
  margin-left: -280rpx;
  text-align: center;
  font-size: 28rpx;
  border-radius: 8rpx;
  color: #000;
}
.icon-success-tip{
  padding-top: 70rpx;
}
.icon-success-tip:before {
  color: #56c538;
  margin-right: 30rpx;
}
.close-withdraw-btn {
  border-top: 1px solid #d2d2d2;
  padding: 30rpx 0;
  color: #ff7100;
  font-size: 34rpx;
  margin-top: 52rpx;
}